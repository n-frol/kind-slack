<?xml version="1.0" encoding="UTF-8"?>
<catalog xmlns="http://www.demandware.com/xml/impex/catalog/2006-10-31" catalog-id="kind-snacks-storefront">

    <category category-id="benefits">
        <template>rendering/category/categorylanding</template>
    </category>

    <category category-id="products">
        <online-flag>true</online-flag>
        <parent>ways-to-snack</parent>
        <template>rendering/category/categorylanding</template>
        <page-attributes>
            <page-title xml:lang="x-default">Search Snacks by Your Favorite Products | KIND Snacks</page-title>
        </page-attributes>
        <custom-attributes>
            <custom-attribute attribute-id="categorySlug" xml:lang="x-default">ways-to-snack/by-product</custom-attribute>
            <custom-attribute attribute-id="enableCompare">false</custom-attribute>
            <custom-attribute attribute-id="showInMenu">false</custom-attribute>
        </custom-attributes>
    </category>

</catalog>
