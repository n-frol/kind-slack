<script>
    $(".youtubemodal").on("click", function() {
        var clone = jQuery(".ytmodal").find(".modalpadding").clone();
        jQuery(".modal_content").html(clone);
        jQuery(".full_modal").show();
        jQuery(".modal_content").show();
        jQuery(".modal_content").css("transform", "none");
        jQuery(".modal_content").css("left", "0");
        jQuery(".modal_content").css("top", "0");
        jQuery(".modal_content").css("width", "100vw");
        jQuery(".modal_content").css("height", "100vh");
    });
    $(document).on('click', '.close_flyout', function() {
        jQuery(".full_modal").hide();
        jQuery(".modal_content").hide();
        console.log("closing frame");
        $('.youtube-iframe').each(function(index) {
            console.log("kill video");
            $(this).attr('src', $(this).attr('src'));
            return false;
        });
    });
</script>

